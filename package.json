{
  "name": "timetrap_tui",
  "version": "0.2.14",
  "description": "Timetrap TUI is a terminal user interface wrapper application for Timetrap.",
  "main": "timetrap_tui.js",
  "bin": {
    "timetrap_tui": "timetrap_tui.js"
  },
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "pretest": "eslint --ignore-path .gitignore .",
    "test": "jest --verbose"
  },
  "jest": {
    "verbose": true,
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/devel/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/karlredman/Timetrap_TUI.git"
  },
  "keywords": [
    "node.js",
    "timetrap",
    "blessed",
    "timetrack",
    "tarminal",
    "interface",
    "tui"
